<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.escrow.smartexamination">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SelectSubject"></activity>
        <activity android:name=".Activity.RecognizeFaceID" />
        <activity android:name=".Activity.ForgetPassword" />
        <activity android:name=".Activity.EmailVarification" />
        <activity android:name=".Activity.Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Registration" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.Paper" />
        <activity android:name=".Adapter.SetUrl" />
        <activity android:name=".Activity.CreateFaceId" />
        <activity android:name=".Activity.DownloadPaper" />
        <activity android:name=".Activity.QuestionList" />
        <activity android:name=".Activity.Submit" />
        <activity android:name=".Activity.DisplayResult" />
    </application>

</manifest>